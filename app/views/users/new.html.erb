<form accept-charset="UTF-8" method="POST" action="/users">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<fieldset>
		Username: <input type="text" name="user[username]" autofocus><br>
		Email: <input type="text" name="user[email]"><br>
		Password: <input type="password" name="user[password]"><br>
		Confirm Password: <input type="password" name="user[password_confirmation]"><br>
		<input type="submit" value='submit'>
	</fieldset>
</form>